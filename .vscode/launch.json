{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C++ Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/src/app/main.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/src/app",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
      "preLaunchTask": "C/C++: g++.exe build active file",
      "miDebuggerServerAddress": "localhost:3333",
      "serverStarted": "/src/app/main.exe",
      "filterStderr": false,
      "filterStdout": false
    }
  ]
}
